{
  "name": "inmoredes-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepare": "husky",
    "format": "prettier --write .",
    "type:check": "tsc --noEmit --pretty",
    "prisma:generate": "prisma generate --schema=./src/db/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=./src/db/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/db/prisma/schema.prisma",
    "prisma:push": "prisma db push --schema=./src/db/prisma/schema.prisma",
    "prisma:dangerously-reset": "prisma migrate reset --force --schema=./src/db/prisma/schema.prisma"
  },
  "dependencies": {
    "@daveyplate/better-auth-ui": "^1.5.12",
    "@hookform/resolvers": "^5.0.1",
