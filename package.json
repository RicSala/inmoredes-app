    "eslint": "^9.26.0",
    "eslint-config-next": "^15.3.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.7.0",
    "prisma-json-types-generator": "^3.3.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5"
